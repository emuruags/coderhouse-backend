   
   <div class="col-12">
       <h1 class="text-center">Productos</h1>
   </div>
   
    

<div class="col-12">
    <div class="mt-3">
        <h5>Agregar nuevo producto</h5>
        <form class="form-inline"  onsubmit="return addProduct()" autocomplete="off">
            <input type="text" name="title" id="title" class="form-control my-2 mr-2" placeholder="TÃ­tulo">
            <input type="text" name="price" id="price" class="form-control my-2 mr-2" placeholder="Precio">
            <input type="text" name="thumbnails" id="thumbnails" class="form-control my-2 mr-2" placeholder="Foto Link">
            <input class="btn btn-primary my-2" type="submit" value="Guardar">
        </form> 
    </div> 
</div>
<br>
<div class="col-12">
    <h5>Lista de Productos</h5>
     <table id="products" class="table mt-3">
        </table>
</div>

<br>


<div class="col-12">
    <div class="mt-3">
    <h5>Centro de Mensajes</h5>
    <form class="form-inline" onsubmit="return addMessage()" autocomplete="off">
      <input type="text" id="email" class="form-control my-2 mr-2" placeholder="Email">
      <input type="text" id="message" class="form-control my-2 mr-2" placeholder="Escribe un mensaje">
      <input class="btn btn-primary my-2" type="submit" value="Enviar">
    </form>
  </div>
</div>
  
  <div class="col-12">
        
        <div id="messages" class="mt-3"></div>
  </div>
    

   
